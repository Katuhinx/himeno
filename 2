# 41 "himenoOACC.c" 2
# 1 "parametr.h"
# 43 "himenoOACC.c"






# 48 "himenoOACC.c"
double second();
float jacobi();
void initmt();
double fflop(int,int,int);
double mflops(int,double,double);

float p[128][128][256];
static float a[4][128][128][256],
              b[3][128][128][256],
              c[3][128][128][256];
static float bnd[128][128][256];
static float wrk1[128][128][256],
              wrk2[128][128][256];

static int imax, jmax, kmax;
static float omega;




int
main()
{
  int i,j,k,nn;
  float gosa;
  double cpu,cpu0,cpu1,flop,target;

  target= 60.0;
  omega= 0.8;
  imax = 128 -1;
  jmax = 128 -1;
  kmax = 256 -1;




  initmt();


  printf("mimax = %d mjmax = %d mkmax = %d\n",128, 128, 256);
  printf("imax = %d jmax = %d kmax =%d\n",imax,jmax,kmax);


  nn= 1000;
  printf(" Start measurement process.\n");
  printf(" Measure the performance in %d times.\n\n",nn);
  printf(" Wait for a while\n\n");

# 95 "himenoOACC.c"
#pragma acc enter data copyin(p, bnd, wrk1, wrk2, a, b, c)
# 95 "himenoOACC.c"





  cpu0 = second();
  gosa = jacobi(nn);
  cpu1 = second();

  cpu= cpu1 - cpu0;
  flop= fflop(imax,jmax,kmax);

  {
  printf(" Loop executed for %d times\n",nn);
  printf(" Gosa : %e \n",gosa);
  printf(" MFLOPS measured : %f.\tcpu : %f\n",mflops(nn,cpu,flop),cpu);
  printf(" Score based on Pentium III 600MHz : %f%d\n", mflops(nn,cpu,flop)/82,84);

  }
  return (0);
}

void
initmt()
{
 int i,j,k;
   for(i=0 ; i<128 ; i++)
    for(j=0 ; j<128 ; j++)
      for(k=0 ; k<256 ; k++){
        a[0][i][j][k]=0.0;
        a[1][i][j][k]=0.0;
        a[2][i][j][k]=0.0;
        a[3][i][j][k]=0.0;
        b[0][i][j][k]=0.0;
        b[1][i][j][k]=0.0;
        b[2][i][j][k]=0.0;
        c[0][i][j][k]=0.0;
        c[1][i][j][k]=0.0;
        c[2][i][j][k]=0.0;
        p[i][j][k]=0.0;
        wrk1[i][j][k]=0.0;
        bnd[i][j][k]=0.0;
      }
  for(i=0 ; i<imax ; i++)
    for(j=0 ; j<jmax ; j++)
      for(k=0 ; k<kmax ; k++){
        a[0][i][j][k]=1.0;
        a[1][i][j][k]=1.0;
        a[2][i][j][k]=1.0;
        a[3][i][j][k]=1.0/6.0;
        b[0][i][j][k]=0.0;
        b[1][i][j][k]=0.0;
        b[2][i][j][k]=0.0;
        c[0][i][j][k]=1.0;
        c[1][i][j][k]=1.0;
        c[2][i][j][k]=1.0;
        p[i][j][k]=(float)(i*i)/(float)((imax-1)*(imax-1));
        wrk1[i][j][k]=0.0;
        bnd[i][j][k]=1.0;
      }

}

float
jacobi(int nn)
{
  int i,j,k,n;
  float gosa, s0, ss;



# 165 "himenoOACC.c"
#pragma acc data present(a, b, c,bnd, wrk1, wrk2,p) create(gosa)
# 165 "himenoOACC.c"



{
    for(n=0 ; n<nn ; ++n){
    gosa = 0.0;

# 171 "himenoOACC.c"
#pragma acc update device(gosa)
# 171 "himenoOACC.c"



# 173 "himenoOACC.c"
#pragma acc parallel loop reduction(+:gosa) collapse(2)
# 173 "himenoOACC.c"


    for(i=1 ; i<imax-1 ; i++){
      for(j=1 ; j<jmax-1 ; j++){

# 177 "himenoOACC.c"
#pragma acc loop reduction(+:gosa) vector
# 177 "himenoOACC.c"

        for(k=1 ; k<kmax-1 ; k++){

          s0 = a[0][i][j][k] * p[i+1][j ][k ]
             + a[1][i][j][k] * p[i ][j+1][k ]
             + a[2][i][j][k] * p[i ][j ][k+1]
             + b[0][i][j][k] * ( p[i+1][j+1][k ] - p[i+1][j-1][k ]
                              - p[i-1][j+1][k ] + p[i-1][j-1][k ] )
             + b[1][i][j][k] * ( p[i ][j+1][k+1] - p[i ][j-1][k+1]
                               - p[i ][j+1][k-1] + p[i ][j-1][k-1] )
             + b[2][i][j][k] * ( p[i+1][j ][k+1] - p[i-1][j ][k+1]
                               - p[i+1][j ][k-1] + p[i-1][j ][k-1] )
             + c[0][i][j][k] * p[i-1][j ][k ]
             + c[1][i][j][k] * p[i ][j-1][k ]
             + c[2][i][j][k] * p[i ][j ][k-1]
             + wrk1[i][j][k];

          ss = ( s0 * a[3][i][j][k] - p[i][j][k] ) * bnd[i][j][k];

          gosa+= ss*ss;


          wrk2[i][j][k] = p[i][j][k] + omega * ss;
        }
      }
    }


# 204 "himenoOACC.c"
#pragma acc parallel loop collapse(2)
# 204 "himenoOACC.c"

    for(i=1 ; i<imax-1 ; ++i){
      for(j=1 ; j<jmax-1 ; ++j){

# 207 "himenoOACC.c"
#pragma acc loop vector
# 207 "himenoOACC.c"

              for(k=1 ; k<kmax-1 ; ++k)
              {p[i][j][k] = wrk2[i][j][k];}
      }
    }


# 213 "himenoOACC.c"
#pragma acc update host(gosa)
# 213 "himenoOACC.c"


   }
 }


  return(gosa);
}



double
fflop(int mx,int my, int mz)
{
  return((double)(mz-2)*(double)(my-2)*(double)(mx-2)*34.0);
}

double
mflops(int nn,double cpu,double flop)
{
  return(flop/cpu*1.e-6*(double)nn);
}

double
second()
{


  struct timeval tm;
  double t ;

  static int base_sec = 0,base_usec = 0;

  gettimeofday(&tm, 
# 246 "himenoOACC.c" 3 4
                   ((void *)0)
# 246 "himenoOACC.c"
                       );

  if(base_sec == 0 && base_usec == 0)
    {
      base_sec = tm.tv_sec;
      base_usec = tm.tv_usec;
      t = 0.0;
  } else {
    t = (double) (tm.tv_sec-base_sec) +
      ((double) (tm.tv_usec-base_usec))/1.0e6 ;
  }

  return t ;
}
